struct RouteDescription
{
    required string highlights(binding="required,max=10000",remark="行程亮点"),
    required string introduce(binding="required,max=10000",remark="线路描述"),
    required string fullyInclusive(binding="required,max=1500",remark="fully Inclusive"),
    required string transportation(binding="required,max=1500",remark="transportation"),
    required string includedMeals(binding="required,max=1500",remark="included Meals"),
    required string groupTour(binding="required,max=1500",remark="group Tour"),
    required string hotelDesc(binding="required,max=1500",remark="hotel Desc"),
    required string physicalRating(binding="required,max=1500",remark="physical Rating"),
}

struct Transportation
{
    required i64 id(binding="required",remark="交通方式id"),
    required string desc(binding="required,max=1500",remark="描述"),
}

struct TransportationResp extends POINT struct Transportation
{
    required string name(remark="名称"),
    required list<struct stable.ImageInfoPair> images(remark="交通工具图片"),
}
struct TransportationInScheduleResp
{
    required i64 id(binding="required",remark="交通方式id"),
    required string name(remark="名称"),
    required list<struct stable.ImageInfoPair> images(remark="交通工具图片"),
}


struct ScheduleInfo
{

    required i32 dayIndex(binding="required",remark="第几天"),
    required string theme(binding="required,max=500",remark="theme"),
    required string themeContent(binding="required,max=10000",remark="theme content"),
    required string sights(binding="required",remark="景点"),
    bool includeTicket(remark="是否包含门票"),
    bool includeGuide(remark="是否包含导游"),
    required string hotel(binding="required,max=255",remark="hotel name"),
    required i16 meals(remark="三餐 bitwise，1 早餐，2 午餐，4 晚餐"),
}

struct ScheduleResp extends POINT struct ScheduleInfo
{
    required i64 id(remark="id"),
    required i64 routId(remark="线路id"),
    required list<struct stable.ImageInfoPair> themeImages(remark="images"),
    required list<struct TransportationInScheduleResp> transportations(remark="天级别交通方式"),
}

struct SnapshotScheduleResp extends POINT struct  ScheduleResp
{
