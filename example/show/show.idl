struct SupplierInfo
{
    i64 id(remark="供应商id"),
    string name(remark="供应商名称"),
    struct stable.ImageInfo logo(remark="供应商logo"),
}

struct LookupRoutePriceReq
{
    required i64 routeId(binding="required",remark="线路id"),
    required i16 routeType(binding="required",remark="线路类型"),
    required string dateStr(binding="required",remark="出团日期"),
}

struct LookupPriceResp
{
    bool isMember(remark="当前用户是否是会员"),
