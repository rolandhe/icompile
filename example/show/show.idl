}

struct DisplayPrice
{
    required i64 price(remark="价格"),
    i32 off(remark="降价百分比,最终展示off%"),
    i64 discount(remark="折后价格"),
}

struct RouteShortShowResp
{
    required i64 id(remark="线路id"),
    required i64 snapshotId(remark="线路snap shot id"),
    required string title(remark="线路title"),
    required string subTitle(remark="线路副标题"),
    required list<i64> cityIds(remark="线路覆盖城市"),
    required i32 routeDays(remark="行程天数"),
    required i64 routeTypes(remark="bitwise模式，支持63种类型，1私家团、2精品团,4定制团"),
    required bool isCityDeep(remark="是否城市深度游"),
